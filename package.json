{
  "name": "osint-profiler",
  "version": "1.0.0",
  "description": "OSINT profiler application for educational purposes",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start"
  },
  "keywords": [
    "osint",
    "profiler"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "exa-js": "^1.10.2",
    "openai": "^6.5.0",
    "supabase": "^2.51.0"
  }
}
